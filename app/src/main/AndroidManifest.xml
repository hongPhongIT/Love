<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pnv.matchmaking.love">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login"></activity>

        <activity android:name=".profile.Profile">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".chat.ChatDetailActivity"></activity>
        <activity android:name=".chat.AllUser" ></activity>
        <activity android:name=".chat.ChatListActivity"></activity>
        <activity android:name=".profile.OtherUserProfile" />
        <activity android:name=".Register" />
    </application>

</manifest>